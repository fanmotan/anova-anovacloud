<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.1//EN"
        "https://raw.githubusercontent.com/gwtproject/gwt/2.6.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='AnovaCloud'>
    <inherits name="com.anova.anovacloud.Common"/>

    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="com.google.gwt.user.Debug"/>
    <set-property name="gwt.logging.logLevel" value="INFO"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>

    <!-- Shared modules -->
    <extend-configuration-property name="gin.ginjector.modules"
                                   value="com.anova.anovacloud.client.gin.DesktopModule"/>
    <extend-configuration-property name="gin.ginjector.modules"
                                   value="com.anova.anovacloud.client.gin.AnovaCloudModule"/>
</module>
