<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'>
<ui:with field='name' type='java.lang.String'/>
<ui:with field='code' type='java.lang.String'/>
<ui:with field='address' type='java.lang.String'/>
<ui:with field='email' type='java.lang.String'/>
<ui:with field='phone' type='java.lang.String'/>
<ui:with field='fax' type='java.lang.String'/>
<ui:with field='status' type='java.lang.String'/>
    

    <ui:style>
        .listItemWrapper {
            padding: 15px 10px;
            font-size: 0.75em;
        }

        .floatLeft {
            float: left;
            font-size: 0.75em;
        }

        .floatRight {
            float: right;
            color: #ababab;
            font-size: 0.75em;
        }

        .clear {
            clear: both;
        }
    </ui:style>

    <div class="{style.listItemWrapper}">
        <div class="{style.floatLeft}">
            <ui:text from="{name}"/>
        </div>
        <div class="{style.floatLeft}">
            <ui:text from="{code}"/>
        </div>
        <div class="{style.floatLeft}">
            <ui:text from="{address}"/>
        </div>
        <div class="{style.floatLeft}">
            <ui:text from="{email}"/>
        </div>
        <div class="{style.floatLeft}">
            <ui:text from="{phone}"/>
        </div>
        
        <div class="{style.floatLeft}">
            <ui:text from="{fax}"/>
        </div>
        
        <div class="{style.floatLeft}">
            <ui:text from="{status}"/>
        </div>
        
        <div class="{style.floatRight}">
            &gt;
        </div>

        <div class="{style.clear}"/>
    </div>
</ui:UiBinder>
